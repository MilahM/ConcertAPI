@model List<Events>

<h2>Events</h2>

<div style="display: block">
    <a href="/Events/ArtistEvents/">Events</a>
</div>

<table class="table">
    <tr>
        <th>Type</th>
        <th>Date_local</th>
        <th>Venue</th>
        <th>Link</th>
    </tr>

    @foreach (var item in @Model)
    {
        <tr>
            <td><a href=/Events/ArtistEvents/@item.Type>@item.Type</a></td>
            <td>@item.Type</td>
            <td>@item.Datetime_local/Time</td>
            <td>@item.Venue</td>
            <td>@item.Url</td>
        </tr>
    }

</table>
